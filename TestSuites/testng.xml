<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite  parallel="tests" name="Selenium Automation Tests">
	<listeners>
		<listener class-name="Ammu.testComponents.Listeners"></listener>
	</listeners>
	<test thread-count="5" name="Submit Order Test">
		<classes>
		<class name="Ammu.tests.StandAloneTestFinal" />
		</classes>
	</test>
	<test thread-count="5" name="Error Validations Test">
		<classes>
		<class name="Ammu.tests.ErrorValidationTest" />
		</classes>
	</test>
</suite> 
